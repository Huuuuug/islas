{
  "compilerOptions": {
    "module": "ESNext",
    "outDir": "dist",
    "rootDir": ".",
    "baseUrl": "src",
    "target": "ESNext",
    "jsx": "react-jsx",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "paths": {
      "shared/*": ["shared/*"],
      "@runtime":["runtime/index.ts"]
    }
  },
  "include": ["src/**/*"],
  "exclude": [
    "**/node_modules/**",
    "**/dist/**",
    "**/playground/**",
    "tsup.config.ts"
  ]
}